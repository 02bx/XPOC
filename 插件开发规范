public class POC_20191121 {
    public String name;
    public String descript;
    public String cve_number;

    public POC_20191121(){
        this.name="test";
        this.descript="struts2反序列化漏洞";
        this.cve_number="CVE_2100_0708";
    }

    public String check(String target){
        boolean flag=false;
        String result;
        if(flag){
            result="存在struts2漏洞呀！"+target;
        }else{
            result="NOT_FIND_BUG";
        }
        return result;
    }
}
